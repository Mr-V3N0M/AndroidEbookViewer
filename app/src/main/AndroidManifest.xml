<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="zkagazapahtnajusz.paperproject.com.paperproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" tools:ignore="ProtectedPermissions" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_SignIn"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_SignUp"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_Home"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_UserProfile"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_BookDetails"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_SearchPage"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_UserProfile_Setting"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_UserProfile_AddFriend"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_Phone_Verification"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_Email_Verification"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_UserPost"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_Comment"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_Friend_Profile"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_PushNotifications"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_SearchStore"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_LibrarySetting"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_StoreSettings"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_Store_More"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Admin.AdminUtils"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_PrivacyAccount_Setting"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.thefinestartist.finestwebview.FinestWebViewActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:screenOrientation="portrait"
            android:theme="@style/FinestWebViewTheme.Light_Custom" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_ForgotPassword"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.ReaderEngine.Reader_PDF"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.ReaderEngine.Reader_TXT"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.ReaderEngine.Reader_EPUB"
            android:screenOrientation="portrait"
            android:theme="@style/CustomWebViewTheme" />
        <!-- Facebook -->
        <meta-data android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id"/>
        <activity android:name="com.facebook.FacebookActivity"
            android:configChanges=
                "keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name" />
        <activity
            android:name="com.facebook.CustomTabActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="@string/fb_login_protocol_scheme" />
            </intent-filter>
        </activity>
        <activity android:name="zkagazapahtnajusz.paperproject.com.paperproject.ReaderEngine.Reader_EPUB_Contents" />
        <activity android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_Dictionary" />

        <!-- Service -->
        <service android:name="zkagazapahtnajusz.paperproject.com.paperproject.Utilities.FirebaseMessagingUtils">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name="zkagazapahtnajusz.paperproject.com.paperproject.Utilities.FirebaseInstanceIdUtils">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_About"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Notification_Home"
            android:screenOrientation="portrait" />
        <activity
            android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_ReportProblem"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name="zkagazapahtnajusz.paperproject.com.paperproject.SchemeCWebView">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:scheme="schemecwebview" />
            </intent-filter>
        </activity>
        <activity android:name="zkagazapahtnajusz.paperproject.com.paperproject.Activity_ActionReceive">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.GET_CONTENT" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="file" />
                <data android:scheme="content" />
                <data android:mimeType="application/epub+zip" />
                <data android:mimeType="application/pdf" />
                <data android:mimeType="text/plain" />
            </intent-filter>
        </activity>
    </application>

</manifest>